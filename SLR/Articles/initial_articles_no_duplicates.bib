  url             = {https://www2.scopus.com/inward/record.uri?eid=2-s2.0-84860423565&doi=10.1109%2fTNS.2011.2158112&partnerID=40&md5=9a9d6307b1bde9babff1a2a798e0dca2},
}

@Conference{Barragan2011,
