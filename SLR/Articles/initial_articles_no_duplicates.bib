@Article{Happe2010,
  author          = {Happe, J. and Becker, S. and Rathfelder, C. and Friedrich, H. and Reussner, R.H.},
  title           = {Parametric performance completions for model-driven performance prediction},
  journal         = {Performance Evaluation},
