}

@Article{BurinDesRoziers2011,
  author          = {Burin Des Roziers, C. and Chelius, G. and Ducrocq, T. and Fleury, E. and Fraboulet, A. and Gallais, A. and Mitton, N. and Nol, T. and Vandaele, J.},
  title           = {Using senslAB* as a first class scientific tool for large scale wireless sensor network experiments},
